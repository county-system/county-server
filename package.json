{
  "name": "backend-server",
  "version": "0.2.5",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon --ignore node_modules/ --ignore public/",
    "db:init": "knex migrate:latest",
    "db:reset": "knex migrate:rollback && knex migrate:latest",
    "db:seed": "knex seed:run",
    "db:migrate": "knex migrate:make",
    "test:local": "nyc --reporter=html mocha test --timeout 10000 --exit",
    "test": "nyc mocha test --timeout 10000 --reporter mocha-junit-reporter --reporter-options mochaFile=coverage/mocha/test-results.xml --exit",
    "lint:js": "eslint .",
    "lint:fix": "eslint . --fix",
    "swagger": "node ./src/config/swagger"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.4.0",
    "@koa/cors": "^3.1.0",
    "@lumieducation/h5p-server": "^8.1.1",
    "accesscontrol": "^2.2.1",
    "async-busboy": "^1.0.1",
    "aws-sdk": "^2.643.0",
    "bcrypt": "^5.0.0",
    "busboy": "^0.3.1",
    "chai-json-schema": "^1.5.1",
    "dotenv": "^10.0.0",
    "faker": "^6.6.6",
    "joi": "^17.2.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.12",
    "koa": "^2.10.0",
    "koa-body": "^4.2.0",
    "koa-bodyparser": "^4.2.1",
    "koa-jwt": "^3.6.0",
    "koa-qs": "^2.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa2-swagger-ui": "^5.3.0",
    "lodash": "^4.17.19",
    "mailgun-js": "^0.22.0",
    "mocha-junit-reporter": "^2.0.0",
    "nanoid": "^3.1.22",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.15",
    "objection": "^2.2.16",
    "objection-soft-delete": "^1.0.7",
    "pg": "^8.0.3",
    "pino": "^6.11.0",
    "pino-pretty": "^4.3.0",
    "rate-limiter-flexible": "^2.0.0",
    "sharp": "^0.28.1",
    "swagger-jsdoc": "^6.1.0",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "2.0.1",
    "eslint": "^6.4.0",
    "mocha": "^7.1.0",
    "nyc": "^14.1.1"
  }
}
